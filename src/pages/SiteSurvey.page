<apex:page standardController="Site_Survey__c" extensions="SiteSurveyExtension" showheader="true" sidebar="false" title="Site Survey" >

	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" ></script>
	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js" ></script>

	<script type="text/javascript">
		j$ = jQuery.noConflict();
		j$(document).ready(function() {
			if('{!siteObject.Approval_Status__c}' == 'Approved'){
				j$(".disabled :input").attr('disabled', true);
				j$(".disabled :input").children().css("opacity", ".5");
				return false;
			} 
		}); 
		
		function checkStatus(){
			if('{!siteObject.Approval_Status__c}' == 'Approved'){
				j$(".disabled :input").attr('disabled', true);
				j$(".disabled :input").children().css("opacity", ".5");
				return false;
			} 
		};
	</script>
     <apex:form >
        <apex:pageBlock >
            <apex:pageMessages />
             	 <apex:pageBlockButtons location="top" style="text-align:center;" >
	                <apex:commandLink value="Cover Sheet" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();" >
	                    <apex:param assignTo="{!tab}" value="Cover Sheet"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Property Info" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();">
	                    <apex:param assignTo="{!tab}" value="Property Info"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Contacts" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();">
	                    <apex:param assignTo="{!tab}" value="Contacts" />
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Media Building Info" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();">
	                    <apex:param assignTo="{!tab}" value="Media Building Info"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="BB MDF" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();">
	                    <apex:param assignTo="{!tab}" value="BB MDF"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="BB IDF" action="{!quickSaveSwitch}" rerender="pageBody"   >
	                    <apex:param assignTo="{!tab}" value="BB IDF"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="BB Connectivity" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();"  >
	                    <apex:param assignTo="{!tab}" value="BB Connectivity"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="TV Info" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();" >
	                    <apex:param assignTo="{!tab}" value="TV Info"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Public TVs" action="{!quickSaveSwitch}" rerender="pageBody"  oncomplete="checkStatus();" >
	                    <apex:param assignTo="{!tab}" value="Public TVs"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Rack Location" action="{!quickSaveSwitch}" rerender="pageBody"  oncomplete="checkStatus();" >
	                    <apex:param assignTo="{!tab}" value="Rack Location"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Property SignOff" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();">
	                	<apex:param assignTo="{!tab}" value="Property SignOff" />
	                </apex:commandLink> &nbsp;&nbsp; 
	                <apex:commandLink value="FTG-GP Lineup" action="{!quickSaveSwitch}" rerender="pageBody" oncomplete="checkStatus();" >
	                    <apex:param assignTo="{!tab}" value="FTG-GP Lineups"/>
	                </apex:commandLink> &nbsp;&nbsp; 
	                <apex:commandLink value="Satellite Info" action="{!quickSaveSwitch}" rerender="pageBody"   oncomplete="checkStatus();">
	                    <apex:param assignTo="{!tab}" value="Satellite Info"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Digital Off-Air" action="{!quickSaveSwitch}" rerender="pageBody"   oncomplete="checkStatus();">
	                    <apex:param assignTo="{!tab}" value="Digital Off-Air"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:commandLink value="Comments" action="{!quickSaveSwitch}" rerender="pageBody"  oncomplete="checkStatus();" >
	                    <apex:param assignTo="{!tab}" value="Comments"/>
	                </apex:commandLink> &nbsp;&nbsp;
	                <apex:outputLink value="{!otherForms}" target="_top">Other Forms</apex:outputLink>
	             </apex:pageBlockButtons>
	        </apex:pageBlock>
            <!-- ****************************************************************************************************************  
            **********************************************  Cover sheet    ******************************************************
            *******************************************************************************************************************-->
        
        <apex:pageBlock id="pageBody" title="{!tab}">
            <apex:pageMessages />
            <apex:pageBlockButtons >
                 <apex:commandButton value="Save and Exit" action="{!saveInfo}" rendered="{!siteObject.Approval_Status__c != 'Approved'}" />&nbsp;&nbsp;
                 <apex:commandButton value="Cancel" action="{!cancel}" />
            </apex:pageBlockButtons>
            <div class="disabled">
            <apex:pageBlockSection columns="1" title="Cover Sheet" rendered="{!tab == 'Cover Sheet' }">
                <apex:pageBlockSection >
	                <apex:outputField label="Site Name" value="{!siteObject.Account_Name__c}"  />
	                <apex:outputField value="{!siteObject.Address__c}" />
	                <apex:outputField value="{!siteObject.OwnerId}" />
                </apex:pageBlockSection>  
                <apex:pageBlockSection columns="1" collapsible="false">
	                <apex:outputText value="Comment Box 1" />
	                <apex:inputField style="width:85%" value="{!siteObject.Main_Comment_Box_1__c}" label="" />
	                <apex:outputText value="Comment Box 2" />
	                <apex:inputField style="width:85%" value="{!siteObject.Main_Comment_Box_2__c}" label="" />
	                <apex:outputText value="Comment Box 3" />
	                <apex:inputField style="width:85%" value="{!siteObject.Main_Comment_Box_3__c}" label="" />
            </apex:pageBlockSection>  
            </apex:pageBlockSection>
            <!-- ****************************************************************************************************************
            **********************************************  Property Info    ************************************************
            *******************************************************************************************************************-->
            
                <apex:pageBlockSection columns="1" title="Property Information" rendered="{!tab == 'Property Info'  }">
                    <apex:pageBlockSection columns="2" collapsible="false" >
                        <apex:outputField value="{!siteObject.Account_Name__c}" />
                        <apex:outputField value="{!siteObject.Address__c}" />
                    </apex:pageBlockSection>
                    <apex:pageBlockSection title="General Info" columns="1">
                    	<apex:pageBlockSection columns="2">    
	                        <apex:outputField value="{!siteObject.Site__r.Phone}"/>
	                        <apex:outputField value="{!siteObject.Site__r.Fax}"/>
                     	</apex:pageBlockSection>
                     	<apex:pageBlockSection columns="1">
                     		<apex:outputPanel >Number of Rooms/Beds <apex:inputField value="{!siteObject.Number_of_Rooms__c}" label=""/> </apex:outputPanel>          
	                        <apex:outputPanel >Total Number of Suites/Semi-Private(2TVs/room) <apex:inputField value="{!siteObject.Total_number_of_Suites_2_TVs_room__c}" label="" /></apex:outputPanel>
	                        <apex:outputPanel >Total Number of Suites/Semi-Private(3TVs/room) <apex:inputField value="{!siteObject.Total_Number_of_Suites_3_TVs_room__c}" label="" /></apex:outputPanel>
	                        <apex:outputPanel >Total Number of Suites/Semi-Private(4TVs/room) <apex:inputField value="{!siteObject.Total_Number_of_Suites_4_TVs_room__c}" label="" /></apex:outputPanel>
	                        <apex:outputPanel >Total # of TVs <apex:inputField value="{!siteObject.Number_of_TVs__c}" label="" /></apex:outputPanel>
                        </apex:pageBlockSection>
                        <apex:pageBlockSection columns="2">
                        	<apex:pageBlockSectionItem >
		                        <apex:selectRadio value="{!siteObject.New_Construction__c}" style="padding-left:140px">
		                            <apex:selectOptions value="{!yesNoList}" />
		                        </apex:selectRadio>
		                        <apex:outputPanel >New Construction</apex:outputPanel>
		                    </apex:pageBlockSectionItem>
	                        <apex:inputField value="{!siteObject.Estimated_Opening_Date__c}" />
	                        <apex:pageBlockSectionItem >
		                        <apex:selectRadio value="{!siteObject.Renovation_Planned__c}" style="padding-left:140px">
		                            <apex:selectOptions value="{!yesNoList}" />
		                        </apex:selectRadio>
		                        <apex:outputPanel >Renovation Planned</apex:outputPanel>
		                    </apex:pageBlockSectionItem>
	                        <apex:inputField value="{!siteObject.Estimated_Completion_Date__c}" />
	                     </apex:pageBlockSection>
	                     <apex:pageBlockSection columns="1" collapsible="false">
	                        <apex:outputPanel >What Areas and What is Scope of Renovation/ Construction<div style="clear:both"></div><apex:inputTextArea style="width:75%" value="{!siteObject.What_Areas_and_What_is_Scope__c}" /></apex:outputPanel>
	                        <apex:inputField style="width:75%" value="{!siteObject.Construction_Company__c}" />
	                     </apex:pageBlockSection>
	                     <apex:pageBlockSection columns="2" collapsible="false"> 
	                        <apex:inputField value="{!siteObject.Construction_POC__c}" />
	                        <apex:inputField value="{!siteObject.Construction_POC_Phone__c}" />
	                        <apex:inputField value="{!siteObject.Owners_Rep__c}" />
	                        <apex:inputField value="{!siteObject.Owners_Rep_Phone__c}" />
	                    </apex:pageBlockSection>
	                </apex:pageBlockSection>
                    <apex:pageBlockSection columns="2" title="Staff Information">
                        <apex:repeat value="{!contactList}" var="con" >
                            <apex:pageBlockSectionItem >
                                <apex:outputField value="{!con.Role__c}" />
                                <apex:outputField value="{!con.Name}" />
                            </apex:pageBlockSectionItem>
                            <apex:outputField value="{!con.Email}" />
                        </apex:repeat>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="2" title="Interface Information">
                        <apex:pageBlockSectionItem >
                        	<apex:selectRadio value="{!siteObject.Interface_iTV_and_Property_PMS_System__c}" style="padding-left:140px">
                                <apex:selectOptions value="{!yesNoList}" />
                            </apex:selectRadio>
                            <apex:outputText value="Is there an interface between an iTV Vendor and the property's PMS System" />
                        </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
	                        <apex:selectRadio value="{!siteObject.ADT_System_in_Use__c}" style="padding-left:140px">
	                            <apex:selectOptions value="{!yesNoList}" />
	                        </apex:selectRadio>
	                        <apex:outputText value="ADT System in Use" />
                        </apex:pageBlockSectionItem>
                        <apex:inputField value="{!siteObject.Software_Vendor_Name__c}" />
                        <apex:inputField value="{!siteObject.Software_Vendor_Phone__c}" />
                        <apex:inputField value="{!siteObject.Onsite_PMS_ADT_Contact__c}" />
                        <apex:inputTextArea cols="75" value="{!siteObject.Describe_Data_Cable_Routing_Location__c}" />
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1" title="PC Admin Menu Access Information">
                    	<apex:outputPanel >Describe the location of the hotel's network (LAN) interface</apex:outputPanel>
                        <apex:inputTextArea style="width:80%" value="{!siteObject.Location_of_Hotel_s_Network_Interface__c}" label="" />
                        <apex:pageBlockSectionItem >
                        	<apex:selectRadio value="{!siteObject.CAT5_Cable_be_Pulled__c}"  style="padding-left:140px">
                                <apex:selectOptions value="{!yesNoList}" />
                            </apex:selectRadio>  
                            <apex:outputText value="Will CAT5 cable need to be pulled?" />
                        </apex:pageBlockSectionItem>
                        <apex:outputPanel >If yes, explain the best possible solution and estimate the distance to the hotel's network (LAN) interface from the SONIFI rack location.<div style="clear:both"></div><apex:inputTextArea style="width:80%" value="{!siteObject.CAT5_Cable_Pull_Solution__c}" label=""/></apex:outputPanel>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection title="Hospitality Property Type Information" columns="1">
                        <apex:pageBlockSectionItem >
                    		<apex:selectRadio value="{!siteObject.Portions_Used_as_Rental_Condo_Timeshares__c}"  style="padding-left:140px">
	                            <apex:selectOptions value="{!yesNoList}" />
	                        </apex:selectRadio>
                        	<apex:outputText value="Are there any portions of the property used as rentals, condos, or timeshare units?" />
                    	</apex:pageBlockSectionItem>
                        <apex:outputPanel >If yes, describe the rental arrangements, room numbers, and unit locations below</apex:outputPanel>
                        <apex:inputTextArea style="width:80%" value="{!siteObject.Describe_Rental_Arrangements_Room_Num__c}" label="" />
                    </apex:pageBlockSection>
                    <apex:pageBlockSection title="Free-To-Guest(FTG) Information" columns="2">
                        <apex:pageBlockSectionItem >
                            <apex:pageBlockSectionItem >
                                <apex:outputPanel > FTG Type&nbsp;&nbsp;&nbsp; </apex:outputPanel>
                                <apex:inputField value="{!siteObject.FTG_Type__c}" />
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputText value="If Other " />
                                <apex:inputText value="{!siteObject.FTG_Type_Other__c}"  />
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSectionItem>
                        <apex:inputText style="display:none" />
                        <apex:inputField value="{!siteObject.Cable_Signal_Type__c}" />
                        <apex:inputField value="{!siteObject.Company_Services_MATV_Sytem__c}" />
                        <apex:inputField value="{!siteObject.FTG_Vendor_Contact__c}" />
                        <apex:inputField value="{!siteObject.MATV_Vendor_Contact__c}" />
                        <apex:inputField value="{!siteObject.FTG_Vendor_Phone__c}" />
                        <apex:inputField value="{!siteObject.MATV_Vendor_Phone__c}" />
                        <apex:pageBlockSectionItem >
                        	<apex:selectRadio value="{!siteObject.FTG_Vendor_Provide_HD_Contact_at_Site__c}" style="padding-left:140px">
                                <apex:selectOptions value="{!yesNoList}" />
                            </apex:selectRadio>
                            <apex:outputText value="FTG Vendor Provides HD Content at Site" />
                        </apex:pageBlockSectionItem>
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1">
                        <apex:inputTextArea style="width:95%" value="{!siteObject.Describe_HD_Equip_at_the_Site__c}" />
                        <apex:inputTextArea style="width:95%" value="{!siteObject.FTG_Comments__c}" />
                        </apex:pageBlockSection>
                    <apex:pageBlockSection columns="2" title="High Speed Internet Information">
                        <apex:pageBlockSectionItem >
                        	<apex:selectRadio value="{!siteObject.Existing_High_Speed_Internet_Provider__c}" style="padding-left:140px">
                                <apex:selectOptions value="{!yesNoList}" />
                            </apex:selectRadio>
                            <apex:outputText value="Is there an existing High Speed Internet Provider?" />
                        </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
                            <apex:pageBlockSectionItem >
                                <apex:outputPanel >If yes, What Type?&nbsp;&nbsp;&nbsp;</apex:outputPanel>
                                <apex:inputField value="{!siteObject.Type_of_High_Speed_Internet_Provider__c}" />
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputText value="If Other " />
                                <apex:inputText value="{!siteObject.Other_Exist_High_Speed_Internet_Provider__c}"  />
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSectionItem>
                        <apex:inputField value="{!siteObject.Circuit_Vendor_Name__c}" />
                        <apex:inputField value="{!siteObject.Circuit_Vendor_Phone__c}" />
                        <apex:inputField value="{!siteObject.Service_Vendor_Contact__c}" />
                        <apex:inputField value="{!siteObject.Service_Vendor_Phone__c}" />
                        <apex:inputField value="{!siteObject.Frequencies_Used_for_Modem_Site__c}" />
                        <apex:inputTextArea cols="75" value="{!siteObject.High_Speed_Internet_Comments__c}" label="Comments" />
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="2" title="Equipment/Shipping Equipment">
                        <apex:inputField value="{!siteObject.Shipping_and_Receiving_Contact__c}" />
                        <apex:inputTextArea cols="75" value="{!siteObject.Special_Shipping_Instructions__c}" />
                        <apex:pageBlockSectionItem >
	                        <apex:selectRadio value="{!siteObject.Lift_Gate_Required_for_Returning_Equip__c}" style="padding-left:140px">
	                            <apex:selectOptions value="{!yesNoList}" />
	                        </apex:selectRadio>
	                        <apex:outputPanel >Lift Gate Required for Returning Equipment</apex:outputPanel>
	                    </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
	                        <apex:selectRadio value="{!siteObject.Loading_Dock_Available__c}" style="padding-left:140px">
	                            <apex:selectOptions value="{!yesNoList}" />
	                        </apex:selectRadio>
	                        <apex:outputPanel >Loading Dock Available</apex:outputPanel>
	                    </apex:pageBlockSectionItem> 
                        <apex:inputTextArea cols="75" value="{!siteObject.Loading_Comments__c}" label="Loading Comments" />
                        <apex:pageBlockSectionItem >
	                        <apex:selectRadio value="{!siteObject.Storage_Space_Available_for_Equipment__c}" style="padding-left:140px">
	                            <apex:selectOptions value="{!yesNoList}" />
	                        </apex:selectRadio>
	                        <apex:outputPanel >Secure Storage Space Available for Equipment</apex:outputPanel>
	                    </apex:pageBlockSectionItem>
                        <apex:inputTextArea cols="75" value="{!siteObject.Storage_Space_Comments__c}" label="Storage Space Comments" />
                    </apex:pageBlockSection>
                    <apex:pageBlockSection columns="1" title="iTV Vendor Information" >
                    	<apex:pageBlockSection columns="2">
	                        <apex:outputPanel ><h3>List the current iTV Vendor or Tape Rental Vendor information below:</h3></apex:outputPanel>
	                        <apex:inputText style="display:none" />
	                        <apex:inputField value="{!siteObject.iTV_Vendor_Name__c}" />
	                        <apex:inputField value="{!siteObject.iTV_Vendor_Phone__c}" />
	                        <apex:inputField value="{!siteObject.Forward_and_Return_Frequencies__c}" />
                        </apex:pageBlockSection>
                        <apex:pageBlockSection columns="1">
                        	<apex:pageBlockSectionItem >
	                            <apex:selectRadio value="{!siteObject.Other_Systems_Onsite__c}" style="padding-left:140px">
	                                <apex:selectOptions value="{!yesNoList}" />
	                            </apex:selectRadio>
	                            <apex:outputText value="Are there any other systems onsite(i.e. tape rental, Robobar, HVAC, etc.)?" />
	                        </apex:pageBlockSectionItem>
	                    </apex:pageBlockSection>
	                    <apex:pageBlockSection columns="2">
	                        <apex:outputPanel ><h3>If Yes, provide the vendor information below.</h3></apex:outputPanel>
	                        <apex:inputText style="display:none" />
	                        <apex:inputField value="{!siteObject.Onsite_Vendor_Name_1__c}" />
	                        <apex:inputField value="{!siteObject.Onsite_Vendor_Phone_1__c}" />
	                        <apex:inputField value="{!siteObject.Onsite_Vendor_Name_2__c}" />
	                        <apex:inputField value="{!siteObject.Onsite_Vendor_Phone_2__c}" />
	                        <apex:inputField value="{!siteObject.Onsite_Forward_and_Return_Frequencies__c}" />
	                        <apex:inputText style="display:none" />
	                        <apex:inputField value="{!siteObject.Existing_HAT_Location_1__c}" />
	                        <apex:inputField value="{!siteObject.Proposed_HAT_Location_1__c}" />
	                        <apex:inputField value="{!siteObject.Existing_HAT_Location_2__c}" />
	                        <apex:inputField value="{!siteObject.Proposed_HAT_Location_2__c}" />
	                        <apex:inputField value="{!siteObject.Existing_HAT_Location_3__c}" />
	                        <apex:inputField value="{!siteObject.Proposed_HAT_Location_3__c}" />
	                    </apex:pageBlockSection>
                        <apex:outputPanel >Describe the proposed HAT #1 Data cable routing and distance</apex:outputPanel>
                        <apex:inputTextArea style="width:85%" value="{!siteObject.Describe_Proposed_HAT_1__c}" label=""/>
                        <apex:outputPanel >Describe the proposed HAT #2 Data cable routing and distance</apex:outputPanel>
                        <apex:inputTextArea style="width:85%" value="{!siteObject.Describe_Proposed_HAT_2__c}" label=""/>
                        <apex:outputPanel >Describe the proposed HAT #3 Data cable routing and distance</apex:outputPanel>
                        <apex:inputTextArea style="width:85%" value="{!siteObject.Describe_Proposed_HAT_3__c}" label=""/>
                        <apex:outputPanel >iTV Vendor Comments</apex:outputPanel>
                        <apex:inputTextArea style="width:85%" value="{!siteObject.iTV_Vendor_Comments__c}" label=""/>
                    </apex:pageBlockSection>
                </apex:pageBlockSection>
            <!-- ****************************************************************************************************************
            **********************************************  CONTACT    ********************************************************** 
            *******************************************************************************************************************-->
            
                <apex:pageBlockSection columns="1" title="Contacts" rendered="{!tab == 'Contacts' }" id="Contacts" >
                    <apex:repeat value="{!contactList}" var="con">
                        <apex:pageBlockSection columns="2" title="{!con.FirstName + ' ' + con.LastName}">
                            <apex:inputField value="{!con.FirstName}" />
                            <apex:inputField value="{!con.LastName}"  required="false" style="border-left-color:#ff0000;border-left-width:5px;"/>
                            <apex:inputField value="{!con.Email}" />
                            <apex:inputField value="{!con.Phone}" />
                            <apex:inputField value="{!con.Role__c}" label="Title/Role" />
                            <apex:inputField value="{!con.MailingStreet}" />
                            <apex:inputField value="{!con.MailingCity}" />
                            <apex:inputField value="{!con.MailingState}" />
                            <apex:inputField value="{!con.MailingPostalCode}" />
                            <apex:inputField value="{!con.MailingCountry}" />
                        </apex:pageBlockSection>
                    </apex:repeat>
                    <apex:commandButton value="Add 1 Contact" action="{!addRowContact}" rerender="pageBody" rendered="{!siteObject.Approval_Status__c != 'Approved'}">
                        <apex:param assignTo="{!numRows}" value="1"/>
                    </apex:commandButton>
                </apex:pageBlockSection>
                
             <!-- ****************************************************************************************************************
            ********************************************** Media Building Info    ************************************************* 
            *******************************************************************************************************************-->
             
                <apex:pageBlockSection columns="1" title="Media Building Info" rendered="{!tab == 'Media Building Info'}" id="Building" >
                    <apex:repeat value="{!buildingMap}" var="build">
                        <apex:pageBlockSection columns="1" title="Building {!buildingMap[build].Name}">
                            <apex:pageBlockSection columns="1" title="Please look at the rooms and fill out the TV & Room Section while in this Building">
                            	<apex:pageBlockSection columns="2">
	                                <apex:inputField value="{!buildingMap[build].Name}" label="Building Name" style="border-left-color:#ff0000;border-left-width:5px;width:60%"/> 
	                                <apex:inputField value="{!buildingMap[build].Number_of_Floors__c}" />
	                                <apex:inputField value="{!buildingMap[build].Number_of_Guest_Patient_Room_Floors__c}" />
	                                <apex:inputField value="{!buildingMap[build].Age_of_Building__c}" />
	                                <apex:inputField value="{!buildingMap[build].Number_of_Cust_room_TV_s_per_floor__c}" label="Number of Guest/Patient TV's per floor"/>
	                                <apex:inputField value="{!buildingMap[build].Number_of_non_Cust_room_TV_s_per_floor__c}" label="Number of non-Guest/Patient TV's per floor"/>
	                                <apex:inputField value="{!buildingMap[build].Length_and_Width_of_Rooms__c}" label="Length and Width of Guest Rooms" />
	                                <apex:inputField value="{!buildingMap[build].Number_of_Meeting_Rooms__c}" />
	                                <apex:inputText value="{!buildingMap[build].Length_of_Hallway__c}" label="Length of Hallway" title="use notes area or floor map if more than one hallway"/>
	                                <apex:inputField value="{!buildingMap[build].Total_Number_of_IDF_s_in_this_building__c}" />
